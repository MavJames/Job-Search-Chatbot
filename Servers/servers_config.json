{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "transport": "stdio",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        ".."
      ]
    },
    "docx_server": {
      "command": "uv",
      "transport": "stdio",
      "args": [
        "run",
        "Servers/docx_server.py"
      ]
    },
    "jobspy-server": {
      "command": "uv",
      "transport": "stdio",
      "args": [
        "run",
        "Servers/jobspy_server.py"
      ]
    }
  }
}